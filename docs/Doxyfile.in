# General information
PROJECT_NAME          = ConnManProxy
OUTPUT_DIRECTORY      = @DOXYGEN_OUTPUT@
OUTPUT_LANGUAGE       = English
TAB_SIZE              = 4 
EXTRACT_ALL           = YES 
OPTIMIZE_OUTPUT_FOR_C = YES 
DOXYFILE_ENCODING     = UTF-8
TYPEDEF_HIDES_STRUCT  = NO 

# Warning and progress messages
QUIET                = YES 
WARNINGS             = YES 
WARN_IF_UNDOCUMENTED = YES 

# Input files
INPUT         = @DOXYGEN_INPUT@
EXCLUDE       = ../include/connman_proxy_internal.h, ../examples, ../src, ../dbus_xml
FILE_PATTERNS = *.h 
RECURSIVE     = YES 

# Output files
GENERATE_HTML  = YES 
GENERATE_LATEX = NO
GENERATE_RTF   = NO
GENERATE_XML   = NO

SOURCE_BROWSER = YES 

# HTML settings
HTML_TIMESTAMP = NO
